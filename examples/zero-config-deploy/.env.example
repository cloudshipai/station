# AI Provider Configuration (required - choose one)
OPENAI_API_KEY=sk-your-openai-key-here
# ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
# GOOGLE_API_KEY=your-google-api-key-here

# AWS Configuration (if using AWS MCP servers)
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1

# E2B Configuration (if using E2B for code execution)
E2B_API_KEY=your-e2b-api-key

# CloudShip Configuration (optional)
STN_CLOUDSHIP_ENABLED=false
STN_CLOUDSHIP_ENDPOINT=lighthouse.cloudshipai.com:50051
STN_CLOUDSHIP_KEY=your-cloudship-key
STN_CLOUDSHIP_STATION_ID=your-station-id

# Station Encryption Key (auto-generated if not provided)
# STATION_ENCRYPTION_KEY=your-custom-encryption-key
